<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bian.mapper.PojoMapper">
	<!-- 查询所有图片 -->
    	<select id="list" resultType="Pojo">
    		select pid, pimage from pojo
    	</select>
    	
    	<insert id="save">
    		insert into pojo(pimage) values(#{pimage})
    	</insert>
</mapper>
